        <type>Default</type>
    </actionOverrides>
    <actionOverrides>
        <actionName>Delete</actionName>
        <type>Default</type>
    </actionOverrides>
    <actionOverrides>
        <actionName>Edit</actionName>
        <type>Default</type>
    </actionOverrides>
    <actionOverrides>
        <actionName>SaveEdit</actionName>
        <type>Default</type>
    </actionOverrides>
    <fields>
        <fullName>Balance__c</fullName>
        <externalId>false</externalId>
        <formula>(Down__c + InstalmentAmount__c)*(1- Discount__c)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Balance</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <type>Currency</type>
    </fields>
    <fields>
        <fullName>Discount__c</fullName>
        <defaultValue>0</defaultValue>
        <externalId>false</externalId>
        <label>Discount</label>
        <precision>2</precision>
        <required>false</required>
        <scale>0</scale>
        <type>Percent</type>
    </fields>
    <fields>
        <fullName>Down__c</fullName>
        <externalId>false</externalId>
        <formula>(UnitPrice *20)/100</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Down</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <type>Currency</type>
    </fields>
    <fields>
        <fullName>InstalmentAmount__c</fullName>
        <externalId>false</externalId>
        <formula>(UnitPrice - Down__c )*1.05</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>InstalmentAmount</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <type>Currency</type>
    </fields>
    <fields>
        <fullName>IsActive</fullName>
    </fields>
    <fields>
        <fullName>Pricebook2Id</fullName>
        <type>Lookup</type>
    </fields>
    <fields>
        <fullName>Product2Id</fullName>
        <type>Lookup</type>
    </fields>
    <fields>
        <fullName>ProductCode</fullName>
    </fields>
    <fields>
        <fullName>StandardPrice</fullName>
    </fields>
    <fields>
        <fullName>UnitPrice</fullName>
    </fields>
    <fields>
        <fullName>UseStandardPrice</fullName>
    </fields>
    <compactLayoutAssignment>SYSTEM</compactLayoutAssignment>
    <enableFeeds>false</enableFeeds>
    <searchLayouts>
        <searchResultsAdditionalFields>PRODUCT.NAME</searchResultsAdditionalFields>
        <searchResultsAdditionalFields>PRODUCT.CUSTOMER_PRODUCT_ID</searchResultsAdditionalFields>
        <searchResultsAdditionalFields>PRODUCT.DEFAULT_PRICE</searchResultsAdditionalFields>
        <searchResultsAdditionalFields>PRODUCT2.DESCRIPTION</searchResultsAdditionalFields>
        <searchResultsAdditionalFields>PRODUCT2.FAMILY_ENUM</searchResultsAdditionalFields>
    </searchLayouts>
    <validationRules>
        <fullName>Discount_Sales</fullName>
        <active>true</active>
        <errorConditionFormula>Discount__c &gt; 0.05</errorConditionFormula>
        <errorMessage>Discount &gt; 5%</errorMessage>
    </validationRules>
</CustomObject>
